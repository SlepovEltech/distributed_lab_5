{"ast":null,"code":"import WebService from \"../services/WebService\";\nexport default {\n  name: 'MainPage',\n  data: function () {\n    return {\n      rate: 0,\n      amount: 0,\n      res: 0\n    };\n  },\n  methods: {\n    setRate: function () {\n      WebService.setRate(this.rate).then(response => {\n        this.rate = response.data;\n        console.log(response.data);\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    convert: function () {\n      WebService.convert(this.amount).then(response => {\n        this.res = response.data;\n        console.log(response.data);\n      }).catch(e => {\n        console.log(e);\n      });\n    }\n  },\n\n  mounted() {\n    this.$nextTick(function () {\n      WebService.getRate().then(response => {\n        this.rate = response.data;\n        console.log(response.data);\n      }).catch(e => {\n        console.log(e);\n      });\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAgBA,OAAOA,UAAP,MAAuB,wBAAvB;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;AAEbC,MAAI,EAAE,YAAY;AAChB,WAAM;AACJC,UAAI,EAAE,CADF;AAEJC,YAAM,EAAE,CAFJ;AAGJC,SAAE,EAAI;AAHF,KAAN;AAKD,GARY;AASbC,SAAO,EAAE;AACPC,WAAO,EAAE,YAAY;AAClBP,gBAAU,CAACO,OAAX,CAAmB,KAAKJ,IAAxB,EACIK,IADJ,CACSC,QAAO,IAAK;AAChB,aAAKN,IAAL,GAAYM,QAAQ,CAACP,IAArB;AACAQ,eAAO,CAACC,GAAR,CAAYF,QAAQ,CAACP,IAArB;AACD,OAJJ,EAKIU,KALJ,CAKUC,KAAK;AACVH,eAAO,CAACC,GAAR,CAAYE,CAAZ;AACD,OAPJ;AAQE,KAVE;AAYPC,WAAO,EAAE,YAAW;AAClBd,gBAAU,CAACc,OAAX,CAAmB,KAAKV,MAAxB,EACKI,IADL,CACUC,QAAO,IAAK;AAChB,aAAKJ,GAAL,GAAWI,QAAQ,CAACP,IAApB;AACAQ,eAAO,CAACC,GAAR,CAAYF,QAAQ,CAACP,IAArB;AACD,OAJL,EAKKU,KALL,CAKWC,KAAK;AACVH,eAAO,CAACC,GAAR,CAAYE,CAAZ;AACD,OAPL;AAQF;AArBO,GATI;;AAgCbE,SAAO,GAAG;AACV,SAAKC,SAAL,CAAe,YAAY;AACzBhB,gBAAU,CAACiB,OAAX,GACKT,IADL,CACUC,QAAO,IAAK;AAChB,aAAKN,IAAL,GAAYM,QAAQ,CAACP,IAArB;AACAQ,eAAO,CAACC,GAAR,CAAYF,QAAQ,CAACP,IAArB;AACD,OAJL,EAKKU,KALL,CAKWC,KAAK;AACVH,eAAO,CAACC,GAAR,CAAYE,CAAZ;AACD,OAPL;AAQD,KATD;AAUF;;AA3Ce,CAAf","names":["WebService","name","data","rate","amount","res","methods","setRate","then","response","console","log","catch","e","convert","mounted","$nextTick","getRate"],"sourceRoot":"","sources":["/home/artem/Documents/currency-front/src/components/MainPage.vue"],"sourcesContent":["<template>\n  <div class=\"main\">\n    <h1>Currency converter</h1>\n\n    <input v-model=\"rate\">\n    <button v-on:click=\"setRate\">Установить rate</button>\n    <br><br>\n    <input v-model=\"amount\">\n    <button v-on:click=\"convert\">Посчитать</button>\n\n    <h3>{{res}}</h3>\n\n  </div>\n</template>\n\n<script>\nimport WebService from \"../services/WebService\";\n\nexport default {\n  name: 'MainPage',\n  data: function () {\n    return{\n      rate: 0,\n      amount: 0,\n      res : 0\n    }\n  },\n  methods: {\n    setRate: function () {\n       WebService.setRate(this.rate)\n          .then(response => {\n            this.rate = response.data;\n            console.log(response.data);\n          })\n          .catch(e => {\n            console.log(e);\n          });\n        },\n\n    convert: function() {\n      WebService.convert(this.amount)\n          .then(response => {\n            this.res = response.data;\n            console.log(response.data);\n          })\n          .catch(e => {\n            console.log(e);\n          });\n    }\n  },\n  mounted() {\n  this.$nextTick(function () {\n    WebService.getRate()\n        .then(response => {\n          this.rate = response.data;\n          console.log(response.data);\n        })\n        .catch(e => {\n          console.log(e);\n        });\n  })\n}\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}